					  "items": [{
						"image": "https://raw.githubusercontent.com/Jasonette/Twitter-UI-example/master/images/home.png",
						"text": "Home",
						"style": {
						  "height": "21"
						},
						"url": "https://raw.githubusercontent.com/tungnapas/qr/master/data.json"
					  }, {
